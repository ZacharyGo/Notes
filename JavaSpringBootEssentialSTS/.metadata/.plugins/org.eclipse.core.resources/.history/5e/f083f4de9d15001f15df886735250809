@Controller
@RequestMapping("/rooms")
public class RoomController {

	private static final List<Room> rooms = new ArrayList<>();
	static {
		for(int i=0; i<10;i++) {
			rooms.add(new Room(i, number: "Room " + i, name: "R"+i, info:"Q"));
		}
	}
	
	@GetMappping
	
	
}
