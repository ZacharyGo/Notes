package com.frankmoley.lil.sbet.landon.roomwebapp.service;

import java.util.ArrayList;
import java.util.List;
import java.util.UUID;

import com.frankmoley.lil.sbet.landon.roomwebapp.mdels.StaffMember;

public class StaffService {
	private static final List<StaffMember> staff = new ArrayList<StaffMember>();
	
	static {
		staff.add(new StaffMember(UUID.randomUUID().toString(), "John", "Doe", "Position"));
		staff.add(new StaffMember(UUID.randomUUID().toString(), "Jane", "Doe", "Position"));
		staff.add(new StaffMember(UUID.randomUUID().toString(), "Oliver", "Handle", "Position"));
		staff.add(new StaffMember(UUID.randomUUID().toString(), "Sammy", "Smith", "Position"));
	}
	
	public List<StaffMember> getAllStaff() {
		return staff;
	}
}
