package com.frankmoley.lil.sbet.landon.roomwebapp.controllers;

import java.util.List;

import org.springframework.web.bind.annotation.GetMapping;
import org.springframework.web.bind.annotation.RequestMapping;
import org.springframework.web.bind.annotation.RestController;

import com.frankmoley.lil.sbet.landon.roomwebapp.mdels.Room;
import com.frankmoley.lil.sbet.landon.roomwebapp.service.StaffService;

@RestController
@RequestMapping("/api/rooms")
public class StaffRestControllre {  //WebService
	private final StaffService staffService;

	public StaffRestControllre(StaffService roomService) {
		this.staffService = roomService;
	}
		
	@GetMapping
	public List<Staff> getAllRooms(){
		return staffService.getAllStaff();
	}
	
}
